<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->


	<bean name="add" class="com.springcore.model.Address">
		<property name="city" value="Hapur" />
		<property name="state" value="UP" />
	</bean>

	<bean name="emp" class="com.springcore.model.Employee" autowire="constructor">
		<constructor-arg name="id" value="111" />
		<constructor-arg name="name" value="Guest" />
		<constructor-arg name="gender" value="Male" />
	</bean>



</beans>