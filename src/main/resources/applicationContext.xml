<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->


	<bean name="add" class="com.springcore.model.Address">
		<property name="city" value="Hapur" />
		<property name="state" value="UP" />
	</bean>

	<bean name="emp" class="com.springcore.model.Employee">
		<property name="id" value="111" />
		<property name="name" value="Guest" />
		<property name="gender" value="Male" />
		<property name="address" ref="add" />
		<property name="list">
			<list>
				<value>20</value>
				<value>220</value>
				<value>150</value>
				<value>220</value>
			</list>
		</property>

		<property name="set">
			<set>
				<value>100</value>
				<value>100</value>
				<value>600</value>
			</set>
		</property>
		<property name="map">
			<map>
				<entry key="1" value="ONE" />
				<entry key="2" value="Two" />
				<entry key="1" value="oneeeee" />
			</map>
		</property>

	</bean>



	<!-- <bean name="emp2" class="com.springcore.model.Employee"> <constructor-arg 
		name="id" value="112" /> <constructor-arg name="name" value="Ayush" /> <constructor-arg 
		name="gender" value="Male" /> <constructor-arg name="address" ref="add"/> 
		</bean> -->

</beans>